# ZTE configuration options

config MACH_ZTE
	bool "ZTE device"
	depends on ARCH_MSM
	help
	  Support for ZTE products

config MACH_ZTE_MSM8996
	bool "ZTE MSM8996"
	depends on ARCH_MSM8996
	select MACH_ZTE
	help
	  Support for MSM8996 ZTE variants

menu "ZTE board selection"

config MACH_ZTE_AILSA_II
	bool "Ailsa board"
	select MACH_ZTE_MSM8996
	help
	  Support for Axon 7 (ailsa)

endmenu
